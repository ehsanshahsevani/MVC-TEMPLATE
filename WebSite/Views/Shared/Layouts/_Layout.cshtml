<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />

	<title>@ViewData["Title"] - WebSite</title>

	<Partial name="~/Views/Shared/PartialViews/_STyleLinks.cshtml" />

	<link rel="stylesheet" href="~/WebSite.styles.css" asp-append-version="true" />

	@await RenderSectionAsync("Styles", required: false)
</head>
<body>
	<Partial name="~/Views/Shared/PartialViews/_Header.cshtml" />

	<div class="container">
		<main role="main" class="pb-3">
			@RenderBody()
		</main>
	</div>

	<Partial name="~/Views/Shared/PartialViews/_Footer.cshtml" />

	<Partial name="~/Views/Shared/PartialViews/_ScriptLinks.cshtml" />

	<script src="~/js/site.js" asp-append-version="true"></script>

	@await RenderSectionAsync("Scripts", required: false)
</body>
</html>
